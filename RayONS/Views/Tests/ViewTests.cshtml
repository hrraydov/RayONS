@model List<RayONS.Models.TestsViewModel>
@{
    ViewBag.Title = "ViewTests";
}

<h2>@ViewBag.SubcategoryName</h2>
@if (Model.Count() != 0)
{
    foreach (var item in Model)
    {
    <a class="testLink" data-target="@item.TestId" href="#">@item.TestName</a><br />
    <br />
    }

    for (int i = 1; i <= (int)ViewData["numberOfPages"]; i++)
    {
    <a class="pageLinkForTest" data-subcategory-id="@ViewBag.SubcategoryId" data-page-number="@i" href="#">@i</a>
    }
}